;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="flyball"
run/main_scene="res://assets/scenes/mainscn.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")

[autoload]

SaveGame="*res://assets/scripts/SaveGame.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="flyball"

[editor]

run/main_run_args="--rendering-driver opengl3"

[editor_plugins]

enabled=PackedStringArray("res://addons/zylann.hterrain/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/Images/": "blue",
"res://assets/Models/": "red",
"res://assets/env/": "yellow",
"res://assets/objects/": "green",
"res://assets/scenes/": "purple",
"res://assets/scripts/": "gray",
"res://assets/sound/": "orange",
"res://assets/texture/": "pink"
}

[filesystem]

import/blender/enabled=false

